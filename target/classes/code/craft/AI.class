   public static Macros DAYS_MACROS(double weightKg, double heightCm, int age, String gender, double durationMin, String sport, String target) throws URISyntaxException {
      double BMR = gender.equalsIgnoreCase("Male") ? 10.0 * weightKg + 6.25 * heightCm - (double)(5 * age) + 5.0 : 10.0 * weightKg + 6.25 * heightCm - (double)(5 * age) - 161.0;
      double caloriesBurned = getMetValue(sport) * 0.0175 * weightKg * durationMin;
      double totalCalories = BMR + caloriesBurned;
      if (target == "Lose weight") {
         totalCalories = BMR * 0.85 + caloriesBurned;
      } else if (target == "Gain muscle") {
         totalCalories = BMR * 1.15 + caloriesBurned;
      }

      double proteinCalories = totalCalories * 0.25;
      double carbCalories = totalCalories * 0.5;
      double fatCalories = totalCalories * 0.25;
      long proteinGrams = Math.round(proteinCalories / 4.0);
      long carbGrams = Math.round(carbCalories / 4.0);
      long fatGrams = Math.round(fatCalories / 9.0);
      long roundedCalories = Math.round(totalCalories / 1.0);
      System.out.println("\u0397\u03bb\u03b9\u03ba\u03af\u03b1: " + age + " \u03b5\u03c4\u03ce\u03bd");
      System.out.println("\u0398\u03b5\u03c1\u03bc\u03af\u03b4\u03b5\u03c2 \u039a\u03b1\u03cd\u03c3\u03b7\u03c2: " + totalCalories + " kcal");
      System.out.println("\u03a0\u03c1\u03c9\u03c4\u03b5\u0390\u03bd\u03b5\u03c2: " + proteinGrams + " \u03b3\u03c1");
      System.out.println("\u03a5\u03b4\u03b1\u03c4\u03ac\u03bd\u03b8\u03c1\u03b1\u03ba\u03b5\u03c2: " + carbGrams + " \u03b3\u03c1");
      System.out.println("\u039b\u03af\u03c0\u03b7: " + fatGrams + " \u03b3\u03c1");
      return new Macros(String.valueOf(age), (double)roundedCalories, (double)proteinGrams, (double)carbGrams, (double)fatGrams, 0.0);
   }

